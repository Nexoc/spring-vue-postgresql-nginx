{"ast":null,"code":"import SidebarLink from './SidebarLink';\nimport { collapsed, toggleSidebar, sidebarWidth } from './state';\nimport { mapGetters } from 'vuex';\nexport default {\n  props: {},\n  components: {\n    SidebarLink\n  },\n  setup() {\n    return {\n      collapsed,\n      toggleSidebar,\n      sidebarWidth\n    };\n  },\n  methods: {\n    logOut() {\n      this.$store.dispatch('auth/logOut').then(() => {\n        this.$router.replace({\n          name: 'signin'\n        });\n      });\n    }\n  },\n  computed: {\n    ...mapGetters({\n      authenticated: 'auth/authenticated',\n      username: 'auth/username',\n      userId: 'auth/userId'\n    })\n  }\n};","map":{"version":3,"names":["SidebarLink","collapsed","toggleSidebar","sidebarWidth","mapGetters","props","components","setup","methods","logOut","$store","dispatch","then","$router","replace","name","computed","authenticated","username","userId"],"sources":["/home/nexoc/JS/frontend/frontend-vue/src/components/sidebar/SideBar.vue"],"sourcesContent":["<script>\nimport SidebarLink from './SidebarLink'\nimport { collapsed, toggleSidebar, sidebarWidth } from './state'\nimport { mapGetters } from 'vuex';\n\nexport default {\n  props: {},\n  components: { SidebarLink },\n  setup() {\n      return { collapsed, toggleSidebar, sidebarWidth }\n  },\n\n  methods: {  \n      logOut() {\n            this.$store.dispatch('auth/logOut')\n            .then(() => {\n                this.$router.replace({\n                    name: 'signin'\n                })\n            })\n      },\n  },\n\n  computed: {  \n      ...mapGetters({\n            authenticated: 'auth/authenticated',\n            username: 'auth/username',\n            userId: 'auth/userId',\n      })\n  },\n}\n</script>\n\n<template>\n  <div class=\"sidebar\" :style=\"{ width: sidebarWidth }\">\n    <span\n      class=\"collapse-icon\"\n      :class=\"{ 'rotate-180': collapsed }\"\n      @click=\"toggleSidebar\"\n    >\n      <i class=\"fas fa-angle-double-left\" />\n    </span>\n    <br>\n\n    <template v-if=\"authenticated\">\n        <h2>\n          <span v-if=\"collapsed\">\n            <div>{{ username.charAt(0).toUpperCase() }}</div>\n            <div>{{ username.charAt(1).toUpperCase() }}</div>          \n          </span>\n          <span v-else>{{ username }}</span>\n        </h2>\n\n        <SidebarLink to=\"/\" icon=\"fas fa-home\">Home</SidebarLink>\n        <SidebarLink to=\"/dashboard\" icon=\"fas fa-columns\">toolbar-menu</SidebarLink>\n        <SidebarLink to=\"/about\" icon=\"fas fa-chart-bar\">Button</SidebarLink>\n        <SidebarLink to=\"/draw\" icon=\"fas fa-users\">draw</SidebarLink>\n        <SidebarLink @click=\"handleLogClick\"  icon=\"fas fa-image\">Log</SidebarLink>\n        <form @submit.prevent=\"logOut\"><button>Logout</button></form>\n    </template>\n\n    <template v-else>\n        <h2>\n          <span v-if=\"collapsed\">\n            <div>No</div>\n            <div>Au</div>\n          </span>\n          <span v-else>Please Auth</span>\n        </h2>\n\n        <SidebarLink to=\"/\" icon=\"fas fa-home\">Home</SidebarLink>\n        <SidebarLink to=\"/signin\" icon=\"fas fa-home\">Sign in</SidebarLink>\n        <SidebarLink to=\"/registration\" icon=\"fas fa-home\">Registration</SidebarLink>             \n    </template>\n  </div>\n</template>\n\n<style>\n:root {\n  --sidebar-bg-color: #dcf8ea;\n  --sidebar-item-hover: #38a169;\n  --sidebar-item-active: #a2a8a5;\n}\n</style>\n\n<style scoped>\n.sidebar {\n  color: rgb(0, 0, 0);\n  background-color: var(--sidebar-bg-color);\n\n  float: left;\n  position: fixed;\n  z-index: 1;\n  top: 0;\n  left: 0;\n  bottom: 0;\n  padding: 0.5em;\n\n  transition: 0.3s ease;\n\n  display: flex;\n  flex-direction: column;\n}\n\n.sidebar h1 {\n  height: 2.5em;\n}\n\n.collapse-icon {\n  position: absolute;\n  top: 0;\n  padding: 0.75em;\n  color: rgba(0, 0, 0, 0.7);\n  transition: 0.2s linear;\n}\n\n.rotate-180 {\n  transform: rotate(180deg);\n  transition: 0.2s linear;\n}\n</style>"],"mappings":"AACA,OAAOA,WAAU,MAAO,eAAc;AACtC,SAASC,SAAS,EAAEC,aAAa,EAAEC,YAAW,QAAS,SAAQ;AAC/D,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,KAAK,EAAE,CAAC,CAAC;EACTC,UAAU,EAAE;IAAEN;EAAY,CAAC;EAC3BO,KAAKA,CAAA,EAAG;IACJ,OAAO;MAAEN,SAAS;MAAEC,aAAa;MAAEC;IAAa;EACpD,CAAC;EAEDK,OAAO,EAAE;IACLC,MAAMA,CAAA,EAAG;MACH,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,aAAa,EACjCC,IAAI,CAAC,MAAM;QACR,IAAI,CAACC,OAAO,CAACC,OAAO,CAAC;UACjBC,IAAI,EAAE;QACV,CAAC;MACL,CAAC;IACP;EACJ,CAAC;EAEDC,QAAQ,EAAE;IACN,GAAGZ,UAAU,CAAC;MACRa,aAAa,EAAE,oBAAoB;MACnCC,QAAQ,EAAE,eAAe;MACzBC,MAAM,EAAE;IACd,CAAC;EACL;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}