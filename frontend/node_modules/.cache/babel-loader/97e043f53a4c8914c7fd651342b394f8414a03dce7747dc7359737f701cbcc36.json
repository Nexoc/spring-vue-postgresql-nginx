etItem(\"vue-drawing-canvas\", JSON.stringify(this.$refs.VueCanvasDrawing.getAllStrokes()));\n      alert(\"Strokes saved, reload your browser to see the canvas with previously saved image\");\n    },\n    removeSavedStrokes() {\n      window.localStorage.removeItem(\"vue-drawing-canvas\");\n      alert(\"Strokes cleared from local storage\");\n    },\n    saveImage() {\n      console.log(\"DrawCanvas.vue -> saveImage -> 90\");\n      // <img :src=\"image\" style=\"border: solid 1px #000000\" />\n\n      const data = {\n        image: this.image\n      };\n      // send to context.js actions insertContentData                  \n      this.$store.dispatch('content/insertContentData', data).then(() => {\n        if (this.requestStatus == 201) {\n          //this.$store.dispatch('folder/findAllFolders', this.userId)\n          //this.$router.push('/') \n          console.log(\"DrawCanvas.vue -> saveImage -> 100\");\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["VueDrawingCanvas","ToolBar","mapActions","name","components","data","initialImage","type","from","x","y","coordinates","color","width","fill","image","eraser","disabled","fillShape","line","strokeType","lineCap","lineJoin","backgroundColor","backgroundImage","watermark","additionalImages","mounted","window","localStorage","JSON","parse","getItem","methods","setImage","event","URL","createObjectURL","target","files","$refs","VueCanvasDrawing","redraw","setWatermarkImage","source","imageStyle","height","getCoordinate","getCoordinates","getStrokes","setItem","stringify","getAllStrokes","alert","removeSavedStrokes","removeItem","saveImage","console","log","$store","dispatch","then","requestStatus"],"sources":["/home/nexoc/JS/frontend/frontend-vue/src/components/draw/DrawCanvas.vue"],"sourcesContent":["<script>\nimport VueDrawingCanvas from \"vue-drawing-canvas\";\nimport ToolBar from './ToolBar.vue';\nimport { mapActions } from 'vuex';\n\nexport default {\n  name: \"ServeDev\",\n  components: {\n    VueDrawingCanvas,\n  },\n  data() {\n    return {\n      initialImage: [\n        {\n          type: \"dash\",\n          from: {\n            x: 262,\n            y: 154,\n          },\n          coordinates: [],\n          color: \"#000000\",\n          width: 5,\n          fill: false,\n        },\n      ],\n      x: 0,\n      y: 0,\n      image: \"\",\n      eraser: false,\n      disabled: false,\n      fillShape: false,\n      line: 5,\n      color: \"#000000\",\n      strokeType: \"dash\",\n      lineCap: \"square\",\n      lineJoin: \"miter\",\n      backgroundColor: \"#FFFFFF\",\n      backgroundImage: null,\n      watermark: null,\n      additionalImages: [],\n    };\n  },\n\n  mounted() {\n    if (\"vue-drawing-canvas\" in window.localStorage) {\n      this.initialImage = JSON.parse(\n        window.localStorage.getItem(\"vue-drawing-canvas\")\n      );\n    }\n  },\n\n  methods: {\n    async setImage(event) {\n      let URL = window.URL;\n      this.backgroundImage = URL.createObjectURL(event.target.files[0]);\n      await this.$refs.VueCanvasDrawing.redraw();\n    },\n    async setWatermarkImage(event) {\n      let URL = window.URL;\n      this.watermark = {\n        type: \"Image\",\n        source: URL.createObjectURL(event.target.files[0]),\n        x: 0,\n        y: 0,\n        imageStyle: {\n          width: 600,\n          height: 400,\n        },\n      };\n      await this.$refs.VueCanvasDrawing.redraw();\n    },\n    getCoordinate(event) {\n      let coordinates = this.$refs.VueCanvasDrawing.getCoordinates(event);\n      this.x = coordinates.x;\n      this.y = coordinates.y;\n    },\n    getStrokes() {\n      window.localStorage.setItem(\n        \"vue-drawing-canvas\",\n        JSON.stringify(this.$refs.VueCanvasDrawing.getAllStrokes())\n      );\n      alert(\n        \"Strokes saved, reload your browser to see the canvas with previously saved image\"\n      );\n    },\n    removeSavedStrokes() {\n      window.localStorage.removeItem(\"vue-drawing-canvas\");\n      alert(\"Strokes cleared from local storage\");\n    },\n\n    saveImage() {\n      console.log(\"DrawCanvas.vue -> saveImage -> 90\")\n      // <img :src=\"image\" style=\"border: solid 1px #000000\" />\n\n      const data = {image: this.image}\n      // send to context.js actions insertContentData                  \n      this.$store.dispatch('content/insertContentData', data)\n      .then(() => {                        \n      if (this.requestStatus == 201) {\n          //this.$store.dispatch('folder/findAllFolders', this.userId)\n          //this.$router.push('/') \n          console.log(\"DrawCanvas.vue -> saveImage -> 100\")\n      }\n      })\n\n    }\n  },\n};\n</script>\n\n<template>\n    <div class=\"flex-row\">\n      <div class=\"source\">\n\n        <p>Canvas:</p>\n        <vue-drawing-canvas\n          ref=\"VueCanvasDrawing\"\n          v-model:image=\"image\"\n          :width=\"600\"\n          :height=\"400\"\n          :stroke-type=\"strokeType\"\n          :line-cap=\"lineCap\"\n          :line-join=\"lineJoin\"\n          :fill-shape=\"fillShape\"\n          :eraser=\"eraser\"\n          :lineWidth=\"line\"\n          :color=\"color\"\n          :background-color=\"backgroundColor\"\n          :background-image=\"backgroundImage\"\n          :watermark=\"watermark\"\n          :initial-image=\"initialImage\"\n          saveAs=\"png\"\n          :styles=\"{\n            border: 'solid 1px #000',\n          }\"\n          :lock=\"disabled\"\n          @mousemove=\"getCoordinate($event)\"\n          :additional-images=\"additionalImages\"\n        />\n        <p>\n          x-axis: <strong>{{ x }}</strong\n          >, y-axis: <strong>{{ y }}</strong>\n        </p>\n\n        <div class=\"button-container\">\n          <button type=\"button\" @click.prevent=\"disabled = !disabled\">\n            <svg\n              xmlns=\"http://www.w3.org/2000/svg\"\n              width=\"16\"\n              height=\"16\"\n             